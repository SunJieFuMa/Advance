<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <solid android:color="@color/theme_primary" />
            <stroke android:width="@dimen/width_1" android:color="@color/theme_primary" />
            <corners android:radius="@dimen/radius_5" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <solid android:color="@color/theme_primary" />
            <stroke android:width="@dimen/width_1" android:color="@color/theme_primary" />
            <corners android:radius="@dimen/radius_5" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/color_blue_94bde3" />
            <stroke android:width="@dimen/width_1" android:color="@color/color_blue_94bde3" />
            <corners android:radius="@dimen/radius_5" />
        </shape>
    </item>
</selector>